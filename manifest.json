{
	"manifest_version": 2,
	"name": "我的助手",
	"version": "1.0.0",
	"description": "我的助手服务",
	"icons": {
		"16": "img/icon.png",
		"48": "img/icon.png",
		"128": "img/icon.png"
	},
	"background": {
		"page": "background.html"
	},
	"browser_action": {
		"default_icon": "img/icon.png",
		"default_title": "我的助手服务"
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"js/jquery-3.2.1.min.js",
				"js/settings.js",
				"js/util.js"
			],
			"css": [
				"css/custom.css"
			],
			"run_at": "document_start"
		}
	],
	"permissions": [
		"contextMenus",
		"tabs",
		"notifications",
		"webRequest",
		"webRequestBlocking",
		"storage",
		"http://*/*",
		"https://*/*"
	]
}